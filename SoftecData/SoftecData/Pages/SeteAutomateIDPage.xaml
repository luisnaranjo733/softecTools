<Page x:Class="SoftecData.Pages.SeteAutomateIDPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SoftecData.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SeteAutomateIDPage">

    <StackPanel>
        <Label Content="Set eAutomate ID" Margin="0,0,0,30" />

        <Label Content="eAutomateID" />
        <TextBox x:Name="eAutomateIDBox" Margin="10,20,10,0" MinHeight="20" />



        <Button x:Name="seteAutomateIDBtn" Margin="0,40,0,0" MinHeight="30" Content="Add" Click="seteAutomateID_Click" />
    </StackPanel>
</Page>
